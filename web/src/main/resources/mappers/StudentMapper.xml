<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.bin.hbnu.dao.StudentMapper">
   <!--    Coursegrade selectCoursegradeByids(Integer sid, Integer tid);-->
<select id="selectCoursegradeByids" resultType="com.bin.hbnu.bean.Coursegrade">
    SELECT * FROM
    coursegrade
    WHENEVER sid = #{sid} AND tid = #{tid}
</select>
</mapper>